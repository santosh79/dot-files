alias gst="git status"
alias lp="ls -p"
alias lrt="ls -lrt"
alias la="ls -al"
alias sc="rails console"
alias ss="rails server"
alias lla="ls -lrt | tail -1"
alias lh='ls -a | grep "^\." | more'
export PATH="/usr/local/bin:$PATH"
source ./dot-git-completion.bash

